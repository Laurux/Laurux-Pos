' Gambas module file

Public Sub Prog_Editeur(Fic As String) As String
  
  Dim $Exec As String
  
  $Exec = "kprinter"
  If Not CheckProg.Chk($Exec) Then $Exec = "evince"
  If Not CheckProg.Chk($Exec) Then $Exec = "okular"
  If Not CheckProg.Chk($Exec) Then $Exec = "atril"
  Shell $Exec & " " & Fic
  
End
